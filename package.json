{
  "name": "simple-react-full-stack",
  "version": "1.0.0",
  "description": "Boilerplate to build a full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && NODE_ENV=production node src/server/index.js",
    "client-debug": "DEBUG='express:*' webpack-dev-server --mode development --devtool inline-source-map --hot",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "NODE_ENV=production nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Nico Roble",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/bigquery": "^2.0.1",
    "@material-ui/core": "^3.9.2",
    "@okta/okta-sdk-nodejs": "^1.2.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "cloudinary": "^1.13.2",
    "cloudinary-react": "^1.1.0",
    "express": "^4.16.3",
    "express-fileupload": "^1.1.1-alpha.2",
    "express-session": "^1.15.6",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "pg": "^7.8.0",
    "pg-hstore": "^2.3.2",
    "react": "^16.8.2",
    "react-dom": "^16.5.2",
    "react-dropzone": "^8.1.0",
    "react-modal": "^3.8.1",
    "react-router-dom": "^4.3.1",
    "react-spinners": "^0.5.1",
    "request": "^2.88.0",
    "sequelize": "^4.42.0",
    "superagent": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "base-href-webpack-plugin": "^2.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^4.0.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.17.3",
    "style-loader": "^0.23.0",
    "system-sleep": "^1.3.6",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3"
  }
}
